
@{ ViewBag.Title = "Add Mummy";

}
@model OperationDigger.Models.Mummy

 *@* adding information about the mummy*@

<h2>Quotes</h2>
<hr />
<div class="container">
    <div class="row">
        <div class="col-md-4 mx-auto">
            <form asp-action="AddQuotes">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="MummyId" />
                <div class="form-group">


                    <label asp-for="Age" class="control-label"></label>
                    <input asp-for="Age" class="form-control" />
                    <span asp-validation-for="Age" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Year" class="control-label"></label>
                    <input asp-for="Year" class="form-control" />
                    <span asp-validation-for="Year" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="DOD" class="control-label"></label>
                    <input asp-for="DOD" class="form-control" />
                    <span asp-validation-for="DOD" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Add Mummy Info" class="btn btn-primary" />

                </div>
            </form>

        </div>
    </div>
</div>




