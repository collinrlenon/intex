@{
    ViewData["Title"] = "Edit Burial";
}

@model Burial


<body class="job-single-page">
    <section class="heading-section section section-on-bg">
        <div class="hero-wrapper">
            <div class="hero-holder"></div>
            <div class="hero-mask-gradient"></div>
        </div><!--//hero-wrapper-->
        <div class="container heading-content">
            <h2 class="headline">@ViewData["Title"]</h2>
            <div class="meta">You can now edit this burial Information</div>
        </div><!--//container-->
    </section><!--//heading-section-->

    <div id="job-single-block">
        <div class="job-details">
            <div class="cta-container text-center">
                <!--Form that will pass the data to the next view -->
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10 mx-auto shadow-lg m-5 rounded" style="border: 3px solid black; background-color: #E4D4C8;">
                            <form asp-action="CreateBurial" class="p-3 pb-7 pt-3">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <!--Row 0-->
                                <div class="form-group row pt-3" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--Burials Heading item-->
                                    <div class="col-sm-3">
                                        <h1 class="col-sm-6" style="width: 100%!important; font-weight:bold">Burials</h1>
                                    </div>

                                    <!--Metrics Heading item-->
                                    <div class="col-sm-3">
                                        <h1 class="col-sm-6" style="width: 100%!important; font-weight:bold">Metrics</h1>
                                    </div>

                                    <!--Observations Heading item-->
                                    <div class="col-sm-3">
                                        <h1 class="col-sm-6" style="width: 100%!important; font-weight:bold">Observations</h1>
                                    </div>
                                </div>
                                <hr />

                                <!--Row 1-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--Burial N/S item-->
                                    <div class="col-sm-3">
                                        <label asp-for="BurialNs" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Burial N/S</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BurialNs" type="text" required>
                                            <option value="">--</option>
                                            <option value="N">N (North)</option>
                                            <option value="S">S (South)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="BurialNs"></span>
                                    </div>

                                    <!--SouthToHead item-->
                                    <div class="col-sm-3">
                                        <label asp-for="SouthToHead" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">South To Head (m)</label>
                                        <input class="form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="SouthToHead" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="SouthToHead"></span>
                                    </div>

                                    <!--AritifactFound2 item-->
                                    <div class="col-sm-3">
                                        <label asp-for="AritifactFound2" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Burial Goods</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="AritifactFound2" type="text" required>
                                            <option value="">--</option>
                                            <option value="Y">Y (Yes)</option>
                                            <option value="N">N (No)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="AritifactFound2"></span>
                                    </div>

                                </div>

                                <!--Row 2-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--Low N/S item-->
                                    <div class="col-sm-3">
                                        <label asp-for="LowNs" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Low N/S</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="LowNs" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="LowNs"></span>
                                    </div>

                                    <!--SouthToFeet item-->
                                    <div class="col-sm-3">
                                        <label asp-for="SouthToFeet" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">South To Feet (m)</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="SouthToFeet" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="SouthToFeet"></span>
                                    </div>

                                    <!--PresIndex item-->
                                    <div class="col-sm-3">
                                        <label asp-for="PreservationIndex" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Preservation Index</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="PreservationIndex" type="text" required>
                                            <option value="">--</option>
                                            <option value="I">I</option>
                                            <option value="II">II</option>
                                            <option value="III">III</option>
                                            <option value="IV">IV</option>
                                            <option value="V">V</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="PreservationIndex"></span>
                                    </div>
                                </div>

                                <!--Row 3-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--High N/S item-->
                                    <div class="col-sm-3">
                                        <label asp-for="HighNs" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">High N/S</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="HighNs" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="HighNs"></span>
                                    </div>

                                    <!--WestToHead item-->
                                    <div class="col-sm-3">
                                        <label asp-for="WestToHead" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">West To Head (m)</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="WestToHead" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="WestToHead"></span>
                                    </div>

                                    <!--HairTaken item-->
                                    <div class="col-sm-3">
                                        <label asp-for="HairTaken" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Hair Taken</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="HairTaken" type="text" required>
                                            <option value="">--</option>
                                            <option value="Y">Y (Yes)</option>
                                            <option value="N">N (No)</option>
                                            <option value="P">P (Pending)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="HairTaken"></span>
                                    </div>
                                </div>

                                <!--Row 4-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--Burial E/W item-->
                                    <div class="col-sm-3">
                                        <label asp-for="BurialEw" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Burial E/W</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BurialEw" type="text" required>
                                            <option value="">--</option>
                                            <option value="E">E (East)</option>
                                            <option value="W">W (West)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="BurialEw"></span>
                                    </div>

                                    <!--WestToFeet item-->
                                    <div class="col-sm-3">
                                        <label asp-for="WestToFeet" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">West To Feet (m)</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="WestToFeet" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="WestToFeet"></span>
                                    </div>

                                    <!--AgeCode item-->
                                    <div class="col-sm-3">
                                        <label asp-for="AgeCode" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Age Code</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="AgeCode" type="text" required>
                                            <option value="">--</option>
                                            <option value="A">A (Adult)</option>
                                            <option value="C">C (Child)</option>
                                            <option value="I">I (Infant)</option>
                                            <option value="N">N (Newborn)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="AgeCode"></span>
                                    </div>
                                </div>

                                <!--Row 5-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--Low E/W item-->
                                    <div class="col-sm-3">
                                        <label asp-for="LowEw" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Low E/W</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="LowEw" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="LowEw"></span>
                                    </div>

                                    <!--Length item-->
                                    <div class="col-sm-3">
                                        <label asp-for="Length" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Length (m)</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="Length" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="Length"></span>
                                    </div>

                                    <!--Photo item-->
                                    <div class="col-sm-3">
                                        <label class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Photo:</label>
                                        <input type="file" name="img" accept="image/*">
                                        <span class="text-danger pl-3"></span>
                                    </div>
                                </div>

                                <!--Row 6-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--High E/W item-->
                                    <div class="col-sm-3">
                                        <label asp-for="HighEw" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">High E/W</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="HighEw" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="HighEw"></span>
                                    </div>

                                    <!--BurialDepth item-->
                                    <div class="col-sm-3">
                                        <label asp-for="BurialDepth" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Burial Depth (m)</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BurialDepth" type="number" step=".01" required />
                                        <span class="text-danger pl-3" asp-validation-for="BurialDepth"></span>
                                    </div>

                                    <!--Field Notes item-->
                                    <div class="col-sm-3">
                                        <label class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Field Notes:</label>
                                        <input type="file" name="img" accept="image/*">
                                        <span class="text-danger pl-3"></span>
                                    </div>
                                </div>

                                <!--Row 7-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--BurialSubplot item-->
                                    <div class="col-sm-3">
                                        <label asp-for="BurialSubplot" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Area</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BurialSubplot" type="text" required>
                                            <option value="">--</option>
                                            <option value="NE">NE (North East)</option>
                                            <option value="NW">NW (North West)</option>
                                            <option value="SE">SE (South East)</option>
                                            <option value="SW">SW (South West)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="BurialSubplot"></span>
                                    </div>

                                    <!--Cluster item-->
                                    <div class="col-sm-3">
                                        <label asp-for="Cluster" class="col-sm-6 col-form-label text-left" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Cluster</label>
                                        <select class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="Cluster" type="text" required>
                                            <option value="">--</option>
                                            <option value="Y">Y (Yes)</option>
                                            <option value="N">N (No)</option>
                                        </select>
                                        <span class="text-danger pl-3" asp-validation-for="Cluster"></span>
                                    </div>

                                    <!--Bone Books item-->
                                    <div class="col-sm-3">
                                        <label class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Bone Books:</label>
                                        <input type="file" name="img" accept="image/*">
                                        <span class="text-danger pl-3"></span>
                                    </div>
                                </div>

                                <!--Row 8-->
                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                    <!--BurialNum item-->
                                    <div class="col-sm-3">
                                        <label asp-for="BurialNum" class="col-sm-6 col-form-label text-left" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Burial Number</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BurialNum" type="number" required />
                                        <span class="text-danger pl-3" asp-validation-for="BurialNum"></span>
                                    </div>

                                    <!--DateExc item-->
                                    <div class="col-sm-3">
                                        <label asp-for="DateExc" class="col-sm-6 col-form-label text-left" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Date</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="DateExc" type="date" required />
                                        <span class="text-danger pl-3" asp-validation-for="DateExc"></span>
                                    </div>

                                    <!--TimeCreatedAt item-->
                                    <div class="col-sm-3">
                                        <label asp-for="TimeCreatedAt" class="col-sm-6 col-form-label text-left" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Time</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="TimeCreatedAt" type="time" required />
                                        <span class="text-danger pl-3" asp-validation-for="TimeCreatedAt"></span>
                                    </div>
                                </div>

                                <!--Row 9-->
                                <div class="form-group row" style="margin-bottom: 10px!important;margin-left:122px!important">
                                    <!--Notes item-->
                                    <div class="col-sm-10">
                                        <label asp-for="Notes" class="col-sm-6 col-form-label text-left" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Field Notes</label>
                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="Notes" type="text" required />
                                        <span class="text-danger pl-3" asp-validation-for="Notes"></span>
                                    </div>
                                </div>


                                <!-- ACCORDION 1 -->


                                <div id="accordion">
                                    <div class="card">
                                        <div class="card-header" id="headingOne">
                                            <h5 class="mb-0">
                                                <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne" style="color:#F89D29;font-size:16px">
                                                    Other Fields (Section 1)
                                                </button>
                                            </h5>
                                        </div>

                                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                                            <div class="card-body">

                                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                                    <!--Gender item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="Gender" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Gender</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="Gender" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="Gender"></span>
                                                    </div>

                                                    <!--HairColor item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="HairColor" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Hair Color</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="HairColor" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="HairColor"></span>
                                                    </div>

                                                    <!--HeadDirection item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="HeadDirection" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Head Direction</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="HeadDirection" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="HeadDirection"></span>
                                                    </div>
                                                </div>

                                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                                    <!--PostcraniaTrauma item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="PostcraniaTrauma" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Postcrania Trauma</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="PostcraniaTrauma" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="PostcraniaTrauma"></span>
                                                    </div>

                                                    <!--FieldBookPgnum item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="FieldBookPgnum" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Field Book Page #</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="FieldBookPgnum" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="FieldBookPgnum"></span>
                                                    </div>

                                                    <!--DexInitials item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="DexInitials" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Dex Initials</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="DexInitials" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="DexInitials"></span>
                                                    </div>
                                                </div>

                                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                                    <!--DecInitials item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="DecInitials" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Dec Initials</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="DecInitials" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="DecInitials"></span>
                                                    </div>

                                                    <!--ByuSample item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="ByuSample" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">BYU Samples</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="ByuSample" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="ByuSample"></span>
                                                    </div>

                                                    <!--BodyAnalysis item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="BodyAnalysis" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Body Analysis</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="BodyAnalysis" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="BodyAnalysis"></span>
                                                    </div>
                                                </div>

                                                <div class="form-group row" style="margin-bottom: 10px!important;justify-content:center!important">
                                                    <!--SkullMag item-->
                                                    <div class="col-sm-3">
                                                        <label asp-for="SkullMag" class="col-sm-6 col-form-label text-right" style="text-align: left !important;width: 100%!important;padding-left:0px!important;font-weight:normal!important">Skull Mag</label>
                                                        <input class="col-sm-6 form-control" style="height: 35px!important; font-size: 14px!important;" asp-for="SkullMag" type="text" />
                                                        <span class="text-danger pl-3" asp-validation-for="SkullMag"></span>
                                                    </div>

                                                    <!--PostcraniaMag item-->
                                                    <div class="col-sm-3">
